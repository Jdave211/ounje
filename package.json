  "dependencies": {
    "@google/generative-ai": "^0.11.5",
    "axios": "^1.6.4",
    "babel-plugin-wildcard": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "expo": "^51.0.8",
    "express": "^4.18.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.4.4",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "openai": "^4.24.1",
    "react": "^18.3.1",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "^3.11.0",
